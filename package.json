{
  "name": "vscode-markdown-script",
  "displayName": "Markdown Script",
  "description": "A vscode extension to enhanced markdown code block",
  "version": "0.0.1",
  "publisher": "axetroy",
  "engines": {
    "vscode": "^1.21.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axetroy/vscode-markdown-script.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axetroy/vscode-markdown-script/issues"
  },
  "homepage": "https://github.com/axetroy/vscode-markdown-script#readme",
  "keywords": [
    "markdown",
    "script",
    "vscode",
    "extension"
  ],
  "categories": [
    "Other"
  ],
  "icon": "resources/icon.png",
  "main": "./out/extension",
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "contributes": {
    "commands": [
      {
        "title": "Run Markdown Script",
        "command": "mdscript.run"
      },
      {
        "title": "Copy script",
        "command": "mdscript.copy"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "mdscript.run",
          "when": "never"
        },
        {
          "command": "mdscript.copy",
          "when": "never"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run lint",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint -p ./ -c tslint.json",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^2.2.42",
    "@types/node": "^9.6.6",
    "conventional-changelog-cli": "^1.3.16",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1",
    "vscode": "^1.1.14"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "process-exists": "^3.1.0",
    "remark": "^9.0.0"
  }
}
